{
  "manifestVersion": 1,
  "id": "querytile-extended",
  "version": "0.1.4",
  "name": "Query Tile Extended",
  "scopes": ["vso.work"],
  "description": "Query Tile extended wih aggregation function",
  "publisher": "aurelien-sudre",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Sample"
  ],
  "content": {
    "license": {
      "path": "license.md"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "widget.html",
      "addressable": true
    },
    {
      "path": "configuration.html",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "categories": [
    "Integrate"
  ],
  "contributions": [
    {
      "id": "querytile-extended-widget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".querytile-extended-widget-Configuration"
      ],
      "properties": {
        "name": "Query Tile Extended",
        "description": "Query Tile Widget extended with aggregation function",
        "uri": "widget.html",
        "isNameConfigurable": true,
        "catalogIconUrl:": "images/logo.png",
        "previewImageUrl": "images/logo.png",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 1
          }
        ],
        "supportedScopes": [
          "project_team"
        ]
      }
    },
    {
      "id": "querytile-extended-widget-configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [
        "ms.vss-dashboards-web.widget-configuration"
      ],
      "properties": {
        "name": "Query Tile Extended Configuration",
        "description": "Query Tile Widget extended with aggregation function",
        "uri": "configuration.html"
      }
    }
  ]
}